{
	"kind": "Pod",
	"apiVersion": "v1",
	"metadata": {
		"name": "artifactory",
		"lables": {
			"server": "prod"
		},
    		"namespace": ""
	},
 	"spec": {
		"containers": [{
			"name": "artifactory",
			"image": "jfrog-docker-registry.bintray.io/jfrog/artifactory-pro:4.0.2",
			"ports": [{
					"containerPort": 8080,
					"hostPort": 8080	
				}]
			}],
		"restartPolicy": "Always",
		"volumes": [{
			"name": "artifactory",
			"gcePersistentDisk": {
				"pdName": "artifactory",
				"fsType": "ext4"
			}
		}]
	}
}
